#include <iostream>

using namespace std;

int main()
{
    int a, b, c, d, sum1, win=0, points;

   for(int i=1; i<=5; i++)
   {
       cin>>a;
       cin>>b;
       cin>>c;
       cin>>d;

       sum1 = a+b+c+d;

       if(sum1>win)
       {
           win=sum1;
           points=i;
       }
    }

    cout<< points << " " << win <<endl;

    return 0;
}
