#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    float c, w, l, total;
    int L;
    cin>> c;
    cin>> L;

    for(int i=0; i < L; i++)
    {
        cin>> w >> l;

        total += (w * l);
    }

    cout<<fixed;
    cout<<setprecision(7)<<total * c<<endl;

    return 0;
}
