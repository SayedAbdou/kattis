#include <iostream>
#include <iomanip>

using namespace std;

int main()
{
    int num;
    double counter=0.0;
    cin>> num;
    
    for (int i=0; i<num; i++)
    {
        double x, y, product;
        cin>> x;
        cin>> y;
        
        product = x * y;
        counter+= product;
        
        
    }
    cout << fixed; 
    cout << setprecision(3) << counter << endl;

return 0;

}
